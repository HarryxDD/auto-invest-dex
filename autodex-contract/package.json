{
  "name": "autodex-contracts",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "test": "hardhat test test/machine.test.ts --network hardhat",
    "test:klaytn": "hardhat test test/klaytn.spec.ts --config klaytn-forked.config.ts --network hardhat",
    "test:bnb": "hardhat test test/bnb.spec.ts --config bnb-forked.config.ts --network hardhat",
    "test:scroll_sepolia": "hardhat test test/scroll_sepolia.spec.ts --config scroll_sepolia-forked.config.ts --network hardhat",
    "test:avax": "hardhat test test/machine.spec.ts --config avaxc-forked.config.ts --network hardhat",
    "test:mantle": "hardhat test test/mantle.spec.ts --config mantle-forked.config.ts --network hardhat",
    "test:coverage": "hardhat coverage --network hardhat",
    "lint": "hardhat compile && eslint . --ext .ts --fix",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "lint:fix": "yarn run lint && yarn run prettier",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.2",
    "@nomicfoundation/hardhat-ethers": "3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "1.0.9",
    "@nomicfoundation/hardhat-toolbox": "3.0.0",
    "@nomicfoundation/hardhat-verify": "1.1.1",
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/contracts-upgradeable": "4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.1.0",
    "@openzeppelin/platform-deploy-client": "^1.51.1",
    "@typechain/ethers-v6": "0.5.0",
    "@typechain/hardhat": "9.0.0",
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "chai": "4.3.6",
    "dotenv": "16.3.1",
    "eslint": "8.50.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-prettier": "5.0.0",
    "ethers": "6.7.1",
    "hardhat": "2.18.0",
    "hardhat-gas-reporter": "1.0.8",
    "husky": "8.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-solidity": "1.1.3",
    "solidity-coverage": "0.8.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typechain": "8.3.1",
    "typescript": "5.2.2",
    "web3": "4.1.2",
    "@types/bun": "latest"
  },
  "dependencies": {
    "@uniswap/universal-router": "1.4.0"
  },
  "module": "index.ts",
  "type": ".cjs"
}